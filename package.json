{
  "name": "events_api",
  "version": "0.0.0",
  "scripts": {
    "build": "node build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "tsc && node -r module-alias/register dist/src/index.js --env=development",
    "start:dev": "nodemon --config nodemon.json",
    "start:test": "nodemon --config nodemon.test.json",
    "migration:run": "tsc && sequelize db:migrate",
    "migration:create": "tsc && npx sequelize migration:create",
    "test": "mocha --require ts-node/register 'tests/**/*.spec.ts' --reporter=spec",
    "coverage": "nyc --reporter=text npm run test --recursive",
    "coverage-html": "nyc --reporter=html npm run test tests/* --recursive"
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@infra": "dist/src/infra",
    "@adapters": "dist/src/adapters",
    "@useCases": "dist/src/use-cases",
    "@entities": "dist/src/entities",
    "@shared": "dist/src/shared",
    "@server": "dist/src/server"
  },
  "dependencies": {
    "@types/cors": "^2.8.10",
    "@types/lodash": "^4.14.170",
    "awilix": "^4.3.4",
    "bcrypt": "^5.0.1",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^3.23.3",
    "http-shutdown": "^1.2.2",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "sequelize": "^5.22.3",
    "underscore": "^1.12.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.47",
    "@types/jasmine": "^3.6.6",
    "@types/jsonfile": "^6.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.34",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "find": "^0.3.0",
    "fs-extra": "^9.1.0",
    "jasmine": "^3.6.4",
    "jsonfile": "^6.1.0",
    "nodemon": "^2.0.7",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^3.9.9",
    "underscore.string": "^3.3.5"
  }
}
